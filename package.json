{
  "name": "sam-tests",
  "version": "0.0.1",
  "scripts": {
    "whoo": "",
    "env": "env",
    "clean": "rimraf dist",
    "prebuild-cmd": "mkdirp dist",
    "build-cmd": "browserify --debug -t babelify -p [ css-modulesify -o dist/main.css --before colorguard --after postcss-cssnext ]",
    "build-dist-cmd": "npm run build-cmd -- -o dist/index.js",
    "build:browser": "npm run build-dist-cmd -- --entry src/browser.js | garnish",
    "build:dev": "npm run build-dist-cmd -- --entry src/dev.js | garnish",
    "build": "npm run build:browser",
    "build:dev:watch": "watch \"npm run build:dev\" src",
    "build:watch": "watch \"npm run build\" src",
    "live": "live-reload index.html dist/ --port 9091",
    "webserver": "ws",
    "start:state": "babel-node ./src/stomp/state_actor.js",
    "start:model": "babel-node ./src/stomp/model_actor.js",
    "start:dev:all": "parallelshell \"npm run start:state\" \"npm run start:model\" \"npm run webserver\" \"npm run live\" \"npm run build:watch\" \"npm run test:watch\"",
    "start:dev": "parallelshell \"npm run webserver\" \"npm run live\" \"npm run build:dev:watch\" \"npm run test:watch\"",
    "start": "parallelshell \"npm run start:state\" \"npm run start:model\" \"npm run webserver\"",
    "test:node": "tape -r babel-register src/**/*.spec.js | faucet",
    "test:browser": "browserify src/browser.spec.js --debug -t babelify -p [ css-modulesify -o dist/main.css --before colorguard --after postcss-cssnext ] | tape-run | faucet",
    "test": "npm run test:node && npm run test:browser",
    "test:watch": "watch \"npm run test\" src"
  },
  "author": "Slađan Ristić",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "latest",
    "babel-plugin-transform-async-generator-functions": "latest",
    "babel-plugin-transform-do-expressions": "latest",
    "babel-plugin-transform-function-bind": "latest",
    "babel-plugin-transform-object-rest-spread": "latest",
    "babel-polyfill": "latest",
    "babel-preset-latest": "latest",
    "babel-register": "latest",
    "babelify": "latest",
    "browserify": "latest",
    "colorguard": "latest",
    "css-modulesify": "latest",
    "faucet": "latest",
    "fetch-ponyfill": "latest",
    "flyd": "latest",
    "garnish": "latest",
    "hyperscript": "latest",
    "jsverify": "latest",
    "live-reload": "latest",
    "local-web-server": "latest",
    "mkdirp": "latest",
    "morphdom": "latest",
    "parallelshell": "latest",
    "postcss-cssnext": "latest",
    "ramda": "latest",
    "rimraf": "latest",
    "sockjs-client": "latest",
    "stompjs": "git://github.com/thoughtwire/stomp-websocket#master",
    "stylelint-config-standard": "latest",
    "tape": "latest",
    "tape-run": "latest",
    "trine": "latest",
    "watch": "latest"
  },
  "devDependencies": {}
}
